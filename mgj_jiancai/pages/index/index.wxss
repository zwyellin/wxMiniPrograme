.redBagCount {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 9;
}

.redBagCountBox {
  width: 598rpx;
  height: 647rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background: url(http://7xu5hi.com1.z0.glb.clouddn.com/201809281757104031575.png) no-repeat;
  background-size: 100%;
  color: #fff;
}

.redBagCountBox .close {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50rpx;
  color: #fff;
  display: flex;
  font-size: 30rpx;
  justify-content: center;
  position: absolute;
  top: 50%;
  left: 0;
  right: 0;
  margin: 0 auto;
  margin-top: 350rpx;
  background: url(http://7xu5hi.com1.z0.glb.clouddn.com/201809221559484833342.png) no-repeat;
  background-size: 100%;
}

.redBagCountBox .span {
  position: absolute;
  display: flex;
  font-size: 30rpx;
  font-weight: bold;
  width: 360rpx;
  height: 80rpx;
  border-radius: 80rpx;
  left: 0;
  right: 0;
  color: #d12c40;
  bottom: 100rpx;
  margin: 0 auto;
  align-items: center;
  justify-content: center;
  background: linear-gradient(#fadc4a, #f5bd41);
}

.redBagCountBox .p {
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 60rpx;
}

.redBagCountBox .money {
  font-size: 40rpx;
  color: #eb4245;
  position: absolute;
  width: 100%;
  left: 0;
  top: 100rpx;
  height: 140rpx;
  padding-top: 50rpx;
  text-align: center;
  display: none;
}

.redBagCountBox .money text {
  font-size: 60rpx;
  font-weight: bold;
  display: block;
  height: 77rpx;
}

.redBagCountBox .money view {
  font-size: 22rpx;
  color: #666;
  width: 220rpx;
  margin: 0 auto;
}

.active .redBagCountBox .money {
  display: block;
}

.active .redBagCountBox {
  background-image: url(http://7xu5hi.com1.z0.glb.clouddn.com/201809141600359205988.png);
}
